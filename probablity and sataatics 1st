import matplotlib  
import matplotlib.pyplot as plt
9%matplotlib inline
y=[5,6,4,3,8,9]
plt.figure(figsize=(20,5))
plt.plot(y)


plt.figure(figsize=(10,5))
plt.title("graph pltting is random")
plt.xlabel("value(X)")
plt.ylabel("value(Y)")
plt.grid()
plt.plot(y)
plt.show()

x=[12,2,3,4,5,6]
plt.scatter(x,y)     #scatter graph

sum(y),sum(x)
sum(x+y) #addition
