#include <iostream>
using namespace std;

int main() {
    int integer1, integer2;
    int num1, num2;
    int sum_even;
    int square_num = 1;  
    int sumSquareOdd = 0;

    cout << "Enter first integer: ";
    cin >> integer1;
    cout << "Enter the second integer: ";
    cin >> integer2;

    do {
        cout << "Enter first num: ";
        cin >> num1;
        cout << "Enter the second num: ";
        cin >> num2;
        cout << "Odd num................" << endl;

        if (num1 % 2 != 0) {
            cout << "Odd num: " << num1 << endl;
        } else {
            num1++;
            cout << "Odd number after increment: " << num1 << endl;
        }

        cout << "Sum of all even number .............." << endl;
        sum_even = 0;

        do {
            int current_num1 = num1;
            if (current_num1 % 2 == 0) {
                sum_even += current_num1;
            }
            num1++;
        } while (num1 < num2);
        cout << "Sum of all even number " << sum_even << endl;

        cout << "Now square of number between 1 and 10........." << endl;
        do {
            cout << "Square of " << square_num << ": " << square_num * square_num << endl;
            square_num++;
        } while (square_num <= 10);

        cout << "Square of odd numbers" << endl;

        do {
            int currentnum2 = num1;
            if (currentnum2 % 2 != 0) {
                sumSquareOdd += currentnum2 * currentnum2;
            }
            num1++;
        } while (num1 <= num2);
        cout << "Sum of the square of odd numbers " << sumSquareOdd << endl;
        break;

    } while (integer1 < integer2);

    char letter = 'A';
    do {
        cout << "Letter......uppercase: " << letter << endl;
        letter++;
    } while (letter <= 'Z');

    cout << endl;
    cout << "Alhamdulilah after 1 hour program finished.....Made by sami....." << endl;
    return 0;
}
