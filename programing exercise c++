// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
int num,even_sum,odd_sum;
while (true)
{
    cout<<"enter a number "<<endl;
 cin>>num;
    cout<<"your number is :"<<num<<endl;
    if (num==0)
    {break;}
    if (num%2==0){
       cout<<"number is   even"<<endl; 
        even_sum+=num;
        
        
    }

    
    else{
        cout<<"number is odd"<<endl;
        odd_sum+=num;
    }
}
cout<<"sum of all the even number is :"<<even_sum<<endl;
cout<<"sum of all the odd number is :"<<odd_sum<<endl;
    return 0;
}
