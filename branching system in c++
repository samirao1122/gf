int main()
{
int marks ;
  cout<<"enter marks :";
    cin>>marks;
if (marks >0 && marks <20){
    cout<<marks; }
else{
  cout<<"not eligible ";
}

#include <iostream>
using namespace std;
struct node{
    
    int val;
   node *next;
    
};
int main() {

  node *n=NULL;
  n =new node;
  n->val=1;
  n->next=NULL;
//   cout<<n->val<<endl;
//   cout<<  n->next<<endl;
    n->next=new node;
      n->next->val=123;
      cout<<  n->next->val<<endl;
      n->next->next=new node;
         n->next->next->val=12456;
         cout<<    n->next->next->val<<endl;
    return 0;
}
